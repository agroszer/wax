<?xml version="1.0" ?>
<resource>
  <NoteBook name="p1">
    <Page text="Simple Panel">
      <VerticalPanel>
        <Button _border="5" _align="c" name="mybutton" text="Click Me!"/>
      </VerticalPanel>
    </Page>
    <Page text="Splitter Test">
      <Splitter direction="v">
        <Button _border="5" _expand="both" text="Click Me [L]"/>
        <Splitter direction="h">
          <Button _border="5" _expand="both" text="Click Me [R1]"/>
          <Button _border="5" _expand="both" text="Click Me [R2]"/>
        </Splitter>
      </Splitter>
    </Page>
    <Page text="Toggle Buttons">
      <VerticalPanel name="p1">
        <ToggleButton _border="5" _expand="both" text="Press Me!"/>
        <ToggleButton _border="5" _expand="both" pressed="1" text="Depress Me!"/>
      </VerticalPanel>
    </Page>
    <Page text="TreeView">
      <TreeListView _border="5" columns="C1,C2,C3" colwidths="400, 75, 75" _expand="both">
        <Root col1="The Root (C1)" col2="The Root (C2)" col3="The Root(C3)" _expanded="1">
          <Item col1="1">
            <Item col1="1,1"/>
            <Item col1="1,2"/>
            <Item col1="1,3" _expanded="1">
              <Item col1="1,3,1"/>
              <Item col1="1,3,2"/>
              <Item col1="1,3,3"/>
              <Item col1="1,3,4"/>
              <Item col1="1,3,5"/>
              <Item col1="1,3,6"/>
              <Item col1="1,3,7"/>
              <Item col1="1,3,8"/>
              <Item col1="1,3,9"/>
            </Item>
          </Item>
          <Item col1="2">
            <Item col1="2,1"/>
            <Item col1="2,2">
              <Item col1="2,2,1"/>
              <Item col1="2,2,2"/>
              <Item col1="2,2,3"/>
            </Item>
          </Item>
          <Item col1="3"/>
        </Root>
      </TreeListView>
    </Page>
    <Page text="ListView">
      <ListView _border="5" columns="C1,C2,C3" colwidths="150,300,150" _expand="both">
        <Item>
          <Col>1,1</Col>
          <Col>1,2</Col>
          <Col>1,3</Col>
        </Item>
        <Item>
          <Col>2,1</Col>
          <Col>2,2</Col>
          <Col>2,3</Col>
        </Item>
        <Item>
          <Col>3,1</Col>
          <Col>3,2</Col>
          <Col>3,3</Col>
        </Item>
        <Item>
          <Col>4,1</Col>
          <Col>4,2</Col>
          <Col>4,3</Col>
        </Item>
        <Item>
          <Col>5,1</Col>
          <Col>5,2</Col>
          <Col>5,3</Col>
        </Item>
      </ListView>
    </Page>
    <Page text="StyledTextBox">
      <StyledTextBox _border="5" _expand="both"/>
    </Page>
    <Page text="FileTreeView">
      <FileTreeView _border="5" _expand="both"/>
    </Page>
    <Page text="RadioButton">
      <HorizontalPanel _border="10">
        <VerticalPanel _border="10">
          <RadioButton text="test 1,1" selected="1" />
          <RadioButton text="test 1,2" />
          <RadioButton text="test 1,3" />
        </VerticalPanel>
        <VerticalPanel _border="10">
          <RadioButton text="test 2,1" />
          <RadioButton text="test 2,2" selected="1" />
          <RadioButton text="test 2,3" />
        </VerticalPanel>
        <VerticalPanel _border="10">
          <RadioButton text="test 3,1" />
          <RadioButton text="test 3,2" />
          <RadioButton text="test 3,3" selected="1" />
        </VerticalPanel>
      </HorizontalPanel>
    </Page>
    <Page text="Bitmaps">
      <VerticalPanel>
        <Bitmap bmp="wtf_face" _align="center" _border="5" />
        <BitmapButton bmp="wtf_face" _align="center" _border="5" />
      </VerticalPanel>
    </Page>
  </NoteBook>
  <Dialog buttonpanel="1" name="mydialog" title="Test">
    <Button _border="5" _expand="both" text="just a test 1"/>
    <Button _border="5" _expand="both" text="just a test 2"/>
  </Dialog>
  <MenuBar name="mb">
    <Menu text="Menu 1">
      <MenuItem text="Menu Item 1"/>
      <MenuItem text="Menu Item 2"/>
      <MenuItem text="Menu Item 3"/>
      <MenuItem enabled="0" text="Menu Item 4 (disabled)"/>
      <Menu text="Submenu 1">
        <MenuItem text="Submenu Item 1"/>
        <MenuItem text="Submenu Item 2"/>
        <MenuItem text="Submenu Item 3"/>
      </Menu>
    </Menu>
    <Menu text="Menu 2">
      <MenuItem checked="1" enabled="0" text="Menu Item 1 (check + checked + disabled)" type="check"/>
      <MenuItem text="Menu Item 2 (check)" type="check"/>
      <MenuItem text="Menu Item 3 (check)" type="check"/>
      <MenuItem checked="1" text="Menu Item 4 (check + checked)" type="check"/>
    </Menu>
    <Menu text="Menu 3">
      <MenuItem text="Menu Item 1 (radio)" type="radio"/>
      <MenuItem text="Menu Item 2 (radio)" type="radio"/>
      <MenuItem text="Menu Item 3 (radio)" type="radio"/>
      <MenuItem text="Menu Item 4 (radio)" type="radio"/>
    </Menu>
  </MenuBar>
  <Image name="wtf_face">
    
  </Image>
</resource>